{
    "version": 2,
    "builds": [
        {
            "src": "kollege_api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/kollege_api/server.js"
        },
        {
            "source": "(.*)",
            "destination": "/index.html"
        }
    ]
}